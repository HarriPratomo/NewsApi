<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.detikcomclone.ui.headline.HeadlineFragment"
        android:label="Headlines"
        tools:layout="@layout/fragment_all_news" />

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.detikcomclone.ui.all_news.Fragment_Allnews"
        android:label="Berita Indonesia"
        tools:layout="@layout/fragment_headlines" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.detikcomclone.ui.source_data.Fragment_All_Data"
        android:label="Sumber Berita"
        tools:layout="@layout/fragment_all_data" />
</navigation>
